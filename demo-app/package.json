{
  "name": "access-checkout-react-native-demo-app",
  "version": "1.0.0",
  "description": "App to demo the access-checkout-react-native-sdk",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "scripts": {
    "android": "npm run build:typescript && react-native run-android",
    "ios": "npm run clean:ios && npm run build:typescript && react-native run-ios",
    "clean:ios": "rm -rf ./ios/build",
    "start": "react-native start",
    "build:typescript": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "format": "prettier 'src/**/*.{ts,tsx,js}' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Worldpay/access-checkout-react-native.git"
  },
  "author": "Worldpay",
  "license": "ISC",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "^0.73.11",
    "@worldpay/access-worldpay-checkout-react-native-sdk": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.13.5",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.2",
    "jest-html-reporter": "^3.10.2",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "react-test-renderer": "18.2.0"
  }
}
