{
  "name": "access-checkout-react-native-demo-app",
  "version": "1.0.0",
  "description": "App to demo the access-checkout-react-native-sdk",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "scripts": {
    "android": "npm run build:typescript && react-native run-android",
    "ios": "npm run build:typescript && react-native run-ios --simulator=\"iphone8-14\"",
    "start": "react-native start",
    "build:typescript": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "format": "prettier 'src/**/*.{ts,tsx,js}' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Worldpay/access-checkout-react-native.git"
  },
  "author": "Worldpay",
  "license": "ISC",
  "dependencies": {
    "@worldpay/access-worldpay-checkout-react-native-sdk": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.71.15"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.66.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.14.8",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "jest-html-reporter": "^3.10.2",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.5.0",
    "typescript": "4.8.4"
  }
}
